+                e.printStackTrace();
+            }
+        }
+    }
+
+    @Override
+    public void onClick(View v) {
+        switch (v.getId()) {
+            case R.id.back_iv:
+                finish();
+                break;
+
+            case R.id.share: {
+                OnekeyShare oks = new OnekeyShare();
+                //关闭sso授权
